// Software to take the information about Dog's name, age, favorite food, height, weight, favorite toy, and owner's name and display its value.

import java.util.Scanner;
public class DogInfo
{
    public static void main(String [] args)
    {
        Scanner in = new Scanner(System.in);
        System.out.println("Welcome to Dog Info ! ");
        System.out.print("Enter the name of the dog : ");
        String dogName = in.next();
        System.out.print("Enter the age of the dog in years : ");
        int dogAge = in.nextInt();
        System.out.print("Enter the favourite food of the dog : ");
        String dogFood = in.next();
        System.out.print("Enter the height of the dog in cm : ");
        int dogHeight = in.nextInt();
        System.out.print("Enter the weight of the dog in kg : ");
        int dogWeight = in.nextInt();
        System.out.print("Enter the favourite toy of the dog : ");
        String dogToy = in.next();
        System.out.print("Enter the name of the dog's owner : ");
        String dogOwner = in.next();
    }

}